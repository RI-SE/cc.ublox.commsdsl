<?xml version="1.0" encoding="UTF-8"?>
<schema name="ublox"
        endian="little"
        nonUniqueMsgIdAllowed="true">
    <platform name="ublox5" />
    <platform name="ublox6" />
    <platform name="ublox7" />
    <platform name="ublox8" />
    
    <fields>
        <enum name="MsgId" type="uint16" semanticType="messageId" hexAssign="true" endian="big">
            <validValue name="NavPosecef" val="0x0101" displayName="NAV-POSECEF" />
            <validValue name="NavPosllh" val="0x0102" displayName="NAV-POSLLH" />
            <validValue name="NavStatus" val="0x0103" displayName="NAV-STATUS" />
            <validValue name="NavDop" val="0x0104" displayName="NAV-DOP" />
            <validValue name="NavAtt" val="0x0105" displayName="NAV-ATT" />
            <validValue name="NavSol" val="0x0106" displayName="NAV-SOL" />
            <validValue name="NavPvt" val="0x0107" displayName="NAV-PVT" />
            <validValue name="NavOdo" val="0x0109" displayName="NAV-ODO" />
            <validValue name="NavResetodo" val="0x0110" displayName="NAV-RESETODO" />
            <validValue name="NavVelecef" val="0x0111" displayName="NAV-VELECEF" />
            <validValue name="NavVelned" val="0x0112" displayName="NAV-VELNED" />
            <validValue name="NavHpposecef" val="0x0113" displayName="NAV-HPPOSECEF" />
            <validValue name="NavHpposllh" val="0x0114" displayName="NAV-HPPOSLLH" />
            <validValue name="NavTimegps" val="0x0120" displayName="NAV-TIMEGPS" />
            <validValue name="NavTimeutc" val="0x0121" displayName="NAV-TIMEUTC" />
            <validValue name="NavClock" val="0x0122" displayName="NAV-CLOCK " />
            <validValue name="NavTimeglo" val="0x0123" displayName="NAV-TIMEGLO" />
            <validValue name="NavTimebds" val="0x0124" displayName="NAV-TIMEBDS" />
            <validValue name="NavTimegal" val="0x0125" displayName="NAV-TIMEGAL" />
            <validValue name="NavTimels" val="0x0126" displayName="NAV-MsgId_NAV_TIMELS" />
            <validValue name="NavSvinfo" val="0x0130" displayName="NAV-SVINFO" />
            <validValue name="NavDgps" val="0x0131" displayName="NAV-DGPS" />
            <validValue name="NavSbas" val="0x0132" displayName="NAV-SBAS" />
            <validValue name="NavOrb" val="0x0134" displayName="NAV-ORB" />
            <validValue name="NavSat" val="0x0135" displayName="NAV-SAT" />
            <validValue name="NavGeofence" val="0x0139" displayName="NAV-GEOFENCE" />
            <validValue name="NavSvin" val="0x013B" displayName="NAV-SVIN" />
            <validValue name="NavRelposned" val="0x013C" displayName="NAV-RELPOSNED" />
            <validValue name="NavEkfstatus" val="0x0140" displayName="NAV-EKFSTATUS" />
            <validValue name="NavAopstatus" val="0x0160" displayName="NAV-AOPSTATUS" />
            <validValue name="NavEoe" val="0x0161" displayName="NAV-EOE" />
        </enum>

    </fields>
    
    <frame name="UbloxFrame">
        <sync name="Sync">
            <field>
                <int name="Sync" type="uint16" defaultValue="0x62b5" validValue="0x62b5" displayName="SYNC"/> 
            </field>
        </sync>
        <id name="Id">
            <field>
                <ref field="MsgId" displayName="CLASS + ID" />
            </field>
        </id>
        <size name="Length">
            <field>
                <int name="LENGTH" type="uint16" /> 
            </field>
        </size>
        <payload name="Payload" />
        <checksum name="Checksum" alg="custom" algName="UbloxChecksum" from="Id" >
            <field>
                <int name="Checksum" type="uint16" displayName="CK" />
            </field>
        </checksum>
    </frame>       
</schema>
